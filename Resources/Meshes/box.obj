# OBJ file format with ext .obj
# vertex count = 8
# face count = 12
v 0.5 0.5 0.5
v -0.5 0.5 0.5
v -0.5 -0.5 0.5
v 0.5 -0.5 0.5
v 0.5 0.5 -0.5
v -0.5 0.5 -0.5
v -0.5 -0.5 -0.5
v 0.5 -0.5 -0.5
# List of texture coordinates, in (u, [v, w]) coordinates, these will vary between 0 and 1. v, w are optional and default to 0.
vt 0 0 [0]
vt 0 1 [0]
vt 1 0 [0]
vt 1 1 [0]
# List of vertex normals in (x,y,z) form; normals might not be unit vectors.
vn 1.0 0.0 0.0
vn 0.0 1.0 0.0
vn 0.0 0.0 1.0
vn -1.0 0.0 0.0
vn 0.0 -1.0 0.0
vn 0.0 0.0 -1.0
# Polygonal face element (see below)
f 1/1/3 2/2/3 3/4/3
f 3/4/3 4/3/3 1/1/3
f 4/3/5 3/4/5 8/1/5
f 8/1/5 3/4/5 7/2/5
f 7/2/4 3/4/4 6/1/4
f 6/1/4 3/4/4 2/3/4
f 2/3/2 1/4/2 6/1/2
f 1/4/2 5/2/2 6/1/2
f 5/2/1 1/4/1 8/1/1
f 8/1/1 1/4/1 4/3/1
f 5/2/6 8/1/6 6/4/6
f 6/4/6 8/1/6 7/3/6